 function getpreviousPageUrl(){
   	 var pu=document.referrer;
   	 console.log("Back Page URL:::::::"+pu);
   //	 window.location.href(pu);
   	 var backlink=document.getElementById('backlink');
   	backlink.location.href=pu;
    }
    function goBack()
    {
    	window.history.back();
    }
    window.onload=function(){
    	getpreviousPageUrl();
    };
