eventRender: function(info) {
            if (info.view.type === 'timeGridWeek') {
                var eventElement = info.el;
                var eventIndex = info.event._def.extendedProps.index; // Assuming you have an index property for each event
                var colorIndex = eventIndex % 3; // Change the number '3' to the number of different colors you want to cycle through
                var colors = ['red', 'green', 'blue']; // Define an array of colors to use
                eventElement.style.backgroundColor = colors[colorIndex];
            }
        },
